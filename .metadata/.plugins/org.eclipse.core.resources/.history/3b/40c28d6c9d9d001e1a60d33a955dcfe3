package clase1;

public class metodos {
	
	/**
	 * Metodo que suma dos numeros reales y devuelve la suma de ellos 
	 * @param a
	 * @param b
	 * @return
	 */
	
	public double suma(double a, double b) {
		return a+b;
	}
	
	/**
	 * Resta dos parametros entero y devuelve dicha resta
	 * @param a
	 * @param b
	 * @return
	 */
	public double resta(double a, double b) {
		return a-b;
	}
	
	/**
	 * Calcula el descuento que queremos al precio que indicamos
	 * @param precio
	 * @param descuento
	 * @return
	 */
	public double descuento(double precio, double descuento) {
		
		return precio - precio*descuento/100;
	}
}
